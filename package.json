{
  "name": "@adambirse123/permission-model",
  "version": "1.1.0",
  "author": "Adam Birse",
  "description": "A centralised permission model",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "postinstall": "husky install",
    "precommit": "npm run prettier && npm run lint:fix && npm run build && npm run test"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.15.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
